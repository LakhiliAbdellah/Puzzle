<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background"
    android:id="@+id/backgroundAcceuil">


    <ImageButton
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/imageButton"
        android:layout_gravity="left|bottom"
        android:clickable="true"
        android:background="@drawable/retour"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="10dp"/>

    <ImageView
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:id="@+id/imageView5"
        android:background="@drawable/text"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="80dp"/>



    <ImageView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:id="@+id/imageView10"
        android:background="@drawable/younes"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="320dp"/>

    <ImageView
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:id="@+id/imageView"
        android:background="@drawable/abdellah"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="360dp"/>
    <ImageView
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:id="@+id/imageView6"
        android:background="@drawable/texte"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="130dp"/>

    <ImageView
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:id="@+id/imageView7"
        android:background="@drawable/textee"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="180dp"/>

    <ImageView
        android:layout_width="340dp"
        android:layout_height="50dp"
        android:id="@+id/imageView8"
        android:background="@drawable/texteee"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="230dp"/>

    <ImageView
        android:layout_width="170dp"
        android:layout_height="39dp"
        android:id="@+id/imageView9"
        android:background="@drawable/texteeee"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="280dp"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:id="@+id/imageView11"
        android:background="@drawable/amine"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="400dp"/>



</FrameLayout>
